{
  "name": "connecthub-frontend",
  "version": "1.0.0",
  "description": "ConnectHub Social Media App Frontend with Dating Features",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=3000",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss src/styles.css -o dist/styles.css",
    "build:js": "webpack src/js/app.js -o dist/app.js",
    "dev": "concurrently \"npm run watch:css\" \"live-server --port=3000\"",
    "watch:css": "postcss src/styles.css -o dist/styles.css --watch"
  },
  "keywords": [
    "social-media",
    "dating-app",
    "web-app",
    "frontend"
  ],
  "author": "ConnectHub Team",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.15",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "socket.io-client": "^4.6.1"
  }
}
